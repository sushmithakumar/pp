###PROXY:SONAR###
    set $upstream_sonar http://sonar.ethan.svc.cluster.local:9000;
    location ~* /sonar {
        rewrite ^/sonar/(.*)$ /$1 break;
        proxy_set_header X-Real-IP  $remote_addr;
        proxy_set_header X-Forwarded-For $remote_addr;
        proxy_set_header Host $host;
        proxy_redirect off;
        proxy_pass $upstream_sonar/sonar;
    }
###PROXY:SONAR###
